# oh-myzsh themes: https://github.com/ohmyzsh/ohmyzsh/wiki/Themes
# ZSH_THEME=robbyrussell

# oh-myzsh plugins
plugins=(
    git
    zsh-autosuggestions
    zsh-syntax-highlighting
)

# Set-up oh-my-zsh
source $ZSH/oh-my-zsh.sh

# zsh history
HISTFILE=~/.zsh_history
HISTSIZE=10000
SAVEHIST=10000
setopt appendhistory
